<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Kanel" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#00e6e6</item>
        <!-- Customize your theme here. -->
        <item name="android:navigationBarColor">#ccffff</item>
        <item name="actionBarStyle">@style/MyActionBar</item>
        <item name="android:windowLightNavigationBar" tools:targetApi="o_mr1">true</item>

    </style>
    <style name="MyActionBar" parent="Widget.AppCompat.Light.ActionBar">

        <item name="background">@color/green_dark</item>

    </style>

</resources>